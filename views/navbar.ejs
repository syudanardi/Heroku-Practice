    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.0/umd/popper.min.js" integrity="sha384-cs/chFZiN24E4KMATLdqdvsezGxaGsi4hLGOzlXwp5UZB1LY//20VyM2taTB4QvJ" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.0/js/bootstrap.min.js" integrity="sha384-uefMccjFJAIv6A+rW+L4AHf99KvxDjWSu1z9VI8SKNVmz4sk7buKt/6v9KI65qnm" crossorigin="anonymous"></script>
    <link href="css/navbar.css" rel="stylesheet" type="text/css">
</head>

<body>

    <nav class="gavbar navbar navbar-light" id="main-navbar" style="background-color: #e3f2fd;">
        <button class="gavbar navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
            <span class="gavbar navbar-toggler-icon"></span>
        </button>

        <!-- Logo -->
        <a class="gavbar navbar-brand" id="navbar-logo" href="#">
            Quick &nbsp
            <img src="resource/logo_bg.png" width="45" height="45" class="gavbar d-inline-block align-top" alt="">
            <!--https://image.ibb.co/heVTHS/logo_bg.png-->
            &nbsp Health
        </a>

        <a href="#" class="gavbar dropdown-toggle" data-toggle="dropdown"><b>Login</b> <span class="gavbar caret"></span></a>
        <ul id="login-dp" class="gavbar dropdown-menu">
            <li>
                <div class="gavbar row">
                    <div class="gavbar col-md-12">
                        Login via
                        <div class="gavbar social-buttons">
                            <a href="#" class="gavbar btn btn-fb"><i class="gavbar fa fa-facebook"></i> Facebook</a>
                            <a href="#" class="gavbar btn btn-tw"><i class="gavbar fa fa-twitter"></i> Twitter</a>
                        </div>
                        or
                        <form class="gavbar form" role="form" method="post" action="login" accept-charset="UTF-8" id="login-nav">
                            <div class="gavbar form-group">
                                <label class="gavbar sr-only" for="exampleInputEmail2">Email address</label>
                                <input type="email" class="gavbar form-control" id="exampleInputEmail2" placeholder="Email address" required>
                            </div>
                            <div class="gavbar form-group">
                                <label class="gavbar sr-only" for="exampleInputPassword2">Password</label>
                                <input type="password" class="gavbar form-control" id="exampleInputPassword2" placeholder="Password" required>
                                <div class="gavbar help-block text-right"><a href="">Forget the password ?</a></div>
                            </div>
                            <div class="gavbar form-group">
                                <button type="submit" class="gavbar btn btn-primary btn-block">Sign in</button>
                            </div>
                            <div class="gavbar checkbox">
                                <label>
                                    <input type="checkbox"> keep me logged-in
                                </label>
                            </div>
                        </form>
                    </div>
                    <div class="gavbar bottom text-center">
                        New to us ? <a href="#"><b>Register Here</b></a>
                    </div>
                </div>
            </li>
        </ul>


        <div class="gavbar collapse navbar-collapse" id="navbarNavAltMarkup">
            <div class="gavbar navbar-nav">
                <a class="gavbar nav-item nav-link active" href="#">Home <span class="gavbar sr-only">(current)</span></a>
                <a class="gavbar nav-item nav-link" href="#">Disease Map</a>
                <a class="gavbar nav-item nav-link" href="#">Disease Wiki</a>
                <a class="gavbar nav-item nav-link disabled" href="#">About Us</a>
            </div>
        </div>

